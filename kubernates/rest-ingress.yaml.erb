apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: rest-ingress
  annotations:
    ngrok.com/ingress-mode: edge
  labels:
    app: rest
spec:
  ingressClassName: ngrok
  rules:
    - host: picked-apt-wombat.ngrok-free.app
      http:
        paths:
          - path: /febe/
            pathType: Prefix
            backend:
              service:
                name: rest-service
                port:
                  number: 80
